{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["post"],
      "route": "experiences"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "CosmosDB",
      "direction": "out",
      "name": "newExperienceBinding",
      "databaseName": "resume",
      "collectionName": "experiences",
      "createIfNotExists": "true",
      "connectionStringSetting": "CosmosDBConnection",
      "partitionKey": "/pk"
    }
  ],
  "scriptFile": "../dist/CreateExperience/index.js"
}
